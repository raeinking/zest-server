<div class="dashboard">
    <form action="/api/posts/doc" class="add Addanimal" enctype="multipart/form-data" method="post">
        <div class="line1">
            <input type="text" name="english" id="english" placeholder="english" required>
            <input type="text" name="kurdish" id="kurdish" placeholder="kurdish" required>
            <input type="text" name="arabic" id="arabic" placeholder="arabic" required>
            <input type="text" name="turkish" id="turkish" placeholder="turkish" required>
            <input type="text" name="badini" id="badini" placeholder="badini" required>
            <select name="gender">
                <option disabled selected value="gender">gender</option>
                <option value="men">men</option>
                <option value="woman">woman</option>
            </select>
            <input type="file" name="myFile" id="myFile" multiple required>
        </div>
        <div class="line2">
            <textarea type="text" name="descriptionenglish" id="description in english"
                placeholder="description in english" required></textarea>
            <textarea type="text" name="descriptionkurdish" id="descriptionkurdish"
                placeholder="descriptionkurdish" required></textarea>
            <textarea type="text" name="descriptionarabic" id="descriptionarabic"
                placeholder="descriptionarabic" required></textarea>
            <textarea type="text" name="descriptionbadini" id="descriptionbadini"
                placeholder="descriptionbadini" required></textarea>
            <textarea type="text" name="descriptionturkish" id="descriptionturkish"
                placeholder="descriptionturkish" required></textarea>
            <input type="text" name="location" id="location" placeholder="location" required>
            <input type="submit" value="submit">
        </div>
    </form>
</div>