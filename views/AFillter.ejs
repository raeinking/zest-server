<div class="dashboard">
    <div class="overview">
        <h1>afiltertype</h1>
        <button><a href="/add_filtertype">Add Filter</a></button>
    </div>
    <div class="boxs-product">
        <table>
            <thead></thead>
            <tr>
                <th>id</th>
                <th>Image</th>
                <th>name</th>
                <th>edit</th>
                <th>delete</th>
            </tr>
            <% products.forEach((product)=> { %>
                <tr>
                    <td>
                        <%= product._id %>
                    </td>
                    <td><img class='smallimg' src="<%= product.image %>"></td>
                    <td>
                        <%= product.english %>
                    </td>
                    <th><a href="/api/filtertype/get/<%=product._id%>">edit</a></th>
                    <th>
                        <form action="/api/filtertype/delete/<%=product._id%>"><input type="submit" value="delete"></form>
                    </th>
                </tr>
                <% }) %>
        </table>
    </div>
</div>